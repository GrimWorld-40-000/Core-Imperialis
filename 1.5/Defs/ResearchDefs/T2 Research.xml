	<!-- Building -->
		
	<ResearchProjectDef ParentName="GW_ImperiumTechBase">
        <defName>GW_IndoctrinationChamber</defName>
        <label>Indoctrination chamber</label>
        <description>Allows for the construction of a Indoctrination chamber. Chamber allow you to recruit "unwavering loyal" pawns with risk of pernament brain damage.</description>
        <prerequisites>
            <li>GW_Cogitator</li>
			<li>Cryptosleep</li>
        </prerequisites>
		<techLevel>Spacer</techLevel>
		<baseCost>4500</baseCost>
		<researchViewX>7.50</researchViewX>
		<researchViewY>1.90</researchViewY>
	</ResearchProjectDef>
	
	<ResearchProjectDef ParentName="GW_ImperiumTechBase">
		<defName>GW_PlasmaReactor</defName>
		<label>plasma reactor</label>
        <description>Allows for the creation of the plasma generator, a massive structure that converts uranium into electricity. Uses uranium to generate massive amount of power.</description>
		<prerequisites>
			<li>GW_Cogitator</li>
		</prerequisites>
		<researchViewX>8.50</researchViewX>
		<researchViewY>0.30</researchViewY>
        <baseCost>7000</baseCost>
        <modExtensions>
            <li Class="GW_Frame.DefModExtension_ExtraPrerequisiteActions">
                <StudyLocation>GW_Cogitator</StudyLocation>
                <ItemStudyRequirements>
                    <li Class="GW_Frame.StudyRequirement">
                        <StudyObject>GW_STC_Fragment</StudyObject>
                        <NumberRequired>2</NumberRequired>
                    </li>    
                </ItemStudyRequirements>
            </li>
        </modExtensions>
	</ResearchProjectDef>
	
	<!-- ??? -->
	
	<ResearchProjectDef ParentName="GW_ImperiumTechBase">
        <defName>GW_Kasrkin</defName>
        <label>karskin</label>
        <description>The Kasrkin in Warhammer 40,000 are elite troops within the Imperial Guard, known for their specialized training and discipline. Adorned in distinctive carapace armor, they excel in various battlefield scenarios. Armed with hot-shot lasguns, they bring formidable firepower to critical missions. Each soldier embodies the Imperium's commitment to producing an elite force for the challenges of the 41st millennium.</description>
        <prerequisites>
            <li>GW_Cogitator</li>
			<li>Cryptosleep</li>
        </prerequisites>
		<techLevel>Spacer</techLevel>
		<baseCost>4500</baseCost>
