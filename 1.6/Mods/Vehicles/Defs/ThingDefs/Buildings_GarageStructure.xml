<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="BuildingBase" Name="GW_GarageDoor" Abstract="True" MayRequire="smashphil.vehicleframework">
		<category>Building</category>
		<altitudeLayer>DoorMoveable</altitudeLayer>
		<fillPercent>1</fillPercent>
		<useHitPoints>true</useHitPoints>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<selectable>true</selectable>
		<tickerType>Normal</tickerType>
		<rotatable>true</rotatable>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<useStuffTerrainAffordance>true</useStuffTerrainAffordance>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>GrimworldCategory</designationCategory>
		<holdsRoof>true</holdsRoof>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<blockLight>true</blockLight>
		<blockWind>true</blockWind>
		<drawerType>RealtimeOnly</drawerType>
		<building>
			<isInert>true</isInert>
			<isAirtight>True</isAirtight>
			<canPlaceOverWall>true</canPlaceOverWall>
			<soundDoorOpenPowered>Door_OpenPowered</soundDoorOpenPowered>
			<soundDoorClosePowered>Door_ClosePowered</soundDoorClosePowered>
			<soundDoorOpenManual>Door_OpenManual</soundDoorOpenManual>
			<soundDoorCloseManual>Door_CloseManual</soundDoorCloseManual>
			<ai_chillDestination>false</ai_chillDestination>
			<paintable>true</paintable>
			<blueprintClass>Vehicles_of_Grimworld.Blueprint_GarageDoor</blueprintClass>
		</building>
		<comps>
			<li Class="CompProperties_Forbiddable">
				<allowNonPlayer>true</allowNonPlayer>
			</li>
		</comps>
		<researchPrerequisites>
			<li>GW_VehiclesOfGrimworld</li>
		</researchPrerequisites>
	</ThingDef>
	
	<ThingDef ParentName="GW_GarageDoor" Name="GW_4x1GarageDoor" MayRequire="smashphil.vehicleframework">
		<defName>GW_4x1GarageDoorClosed</defName>
		<label>Imperialis garage door (4x1)</label>
		<description>Divides rooms. Must be manually opened with pawn interaction allowing vehicles through.</description>
		<thingClass>Vehicles_of_Grimworld.GarageDoor</thingClass>
		<passability>Impassable</passability>
		<defaultPlacingRot>South</defaultPlacingRot>
		<size>(4, 1)</size>
		<costStuffCount>160</costStuffCount>
		<costList>
			<HP_Ceramite>10</HP_Ceramite>
		</costList>
		<statBases>
			<MaxHitPoints>700</MaxHitPoints>
			<WorkToBuild>2000</WorkToBuild>
			<Flammability>1</Flammability>
		</statBases>
		<graphicData>
			<texPath>Things/Building/GarageDoors/GW_GarageDoor_4x1</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1,1)</drawSize>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/GarageDoors/GW_GarageDoor_UI</uiIconPath>
	</ThingDef>

	<ThingDef ParentName="GW_4x1GarageDoor" MayRequire="smashphil.vehicleframework">
		<defName>GW_4x1GarageDoorOpen</defName>
		<passability>Standable</passability>
		<fillPercent>0</fillPercent>
		<blockLight>false</blockLight>
		<researchPrerequisites IsNull="True"/>
		<designationCategory IsNull="True"/>
	</ThingDef>

